{
  "cSpell.words": [
    "Antd",
    "Codeium",
    "daisyui",
    "fontawesome",
    "fortawesome",
    "fullhd",
    "Parens",
    "rushstack",
    "tailwindcss",
    "VITE",
    "windicss",
    "yangchuran"
  ],
  "cSpell.ignoreRegExpList": ["\\\\node_modules\\\\", "\\\\dist\\\\", "\\\\test\\\\", "\\\\.vscode\\\\"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "prettier.configPath": ".prettierrc",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "eslint.options": {
    "configFile": ".eslintrc.js"
  }
}
