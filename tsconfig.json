{
  "extends": "@vue/tsconfig",
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "Bundler",
    "types": [
      "unplugin-vue-router/client",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "sourceMap": true
  },
  "include": [
    "vite.config.*",
    "env.d.ts",
    "src/typed-router.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
}