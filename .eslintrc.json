{
  "extends": "@antfu",
  "ignorePatterns": [
    ".github/**/*",
    "node_modules/**/*",
    ".vscode/**/*",
    "dist/**/*",
    "compose-dev.yaml"
  ],
  "rules": {
    "@typescript-eslint/quotes": ["error", "double"], // 双括号
    "@typescript-eslint/semi": ["error", "always"], // 分号
    "no-console": "warn",
    "curly": ["warn", "multi-or-nest"] // https://eslint.org/docs/rules/curly 花括号报错
  }
}
