{
  "name": "yangchuran",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 4173",
    "lint": "pnpm eslint . --ext .ts,.vue,.json",
    "lint:fix": "pnpm eslint . --fix --ext .ts,.vue,.json",
    "format": "prettier --write \"**/*.{js,ts,vue,json,md,css,less,scss}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,md,css,less,scss}\"",
    "type-check": "vue-tsc --noEmit",
    "dep:check": "pnpm dlx npm-check-updates",
    "dep:update": "pnpm dlx npm-check-updates -u && pnpm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0",
    "axios": "^1.13.2",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@iconify/vue": "^5.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-format": "^1.0.2",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "less": "^4.4.2",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.1",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.18.0",
    "vite": "^7.2.4",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^3.1.5"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,md,css,less,scss}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
